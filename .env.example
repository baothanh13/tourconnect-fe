# TourConnect Frontend Environment Variables
# Copy this file to .env and fill in your actual values

# Application Settings
REACT_APP_APP_NAME=TourConnect
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_API_URL=http://localhost:5000

# Google Maps API
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# OpenAI API (for AI features)
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here

# MoMo Payment Gateway Configuration
REACT_APP_MOMO_PARTNER_CODE=your_momo_partner_code
REACT_APP_MOMO_ACCESS_KEY=your_momo_access_key
REACT_APP_MOMO_SECRET_KEY=your_momo_secret_key
REACT_APP_MOMO_ENDPOINT=https://test-payment.momo.vn/v2/gateway/api/create

# Additional API Keys (if needed)
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_public_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id

# Development Settings
GENERATE_SOURCEMAP=false
SKIP_PREFLIGHT_CHECK=true
